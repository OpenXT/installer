. ${SCRIPT_DIR}/functions
. ${SCRIPT_DIR}/Functions/subgraph

not_previous || exit ${Previous}

run_subgraph "install-main" <<EOF
PACKAGES_DIR='${INSTALL_SOURCE_DIR}/${PACKAGES_DOT_MAIN}'
NETWORK_NIC_CONF='${NETWORK_NIC_CONF}'
NETWORK_STATUS_CONF='${NETWORK_STATUS_CONF}'
EXTRA_OEM_PARTITIONS='${EXTRA_OEM_PARTITIONS}'
EOF

OPT=$?
exit ${OPT}
